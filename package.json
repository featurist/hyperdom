{
  "name": "plastiq",
  "version": "1.17.1",
  "description": "a small and beautiful front-end MVC framework",
  "main": "index.js",
  "dependencies": {
    "virtual-dom": "2.0.1"
  },
  "devDependencies": {
    "bluebird": "^2.9.12",
    "browserify": "9.0.3",
    "chai": "2.0.0",
    "jquery": "^2.1.3",
    "jquery-sendkeys": "^1.0.2",
    "jsdom": "3.1.2",
    "karma-browserify": "3.0.3",
    "karma-chrome-launcher": "^0.1.7",
    "karma-cli": "0.0.4",
    "karma-firefox-launcher": "^0.1.4",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "1.0.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-sauce-launcher": "^0.2.10",
    "mocha": "^2.1.0",
    "trytryagain": "^0.1.0",
    "uglify-js": "^2.4.16",
    "vdom-to-html": "^2.0.0",
    "watchify": "^2.2.1"
  },
  "scripts": {
    "test": "npm run karma && npm run mocha",
    "karma": "karma start --single-run",
    "ci": "karma start karma.conf-ci.js --single-run",
    "mocha": "mocha test/server/*Spec.js",
    "prepublish": "browserify browser.js > plastiq.js && uglifyjs --compress warnings=false --mangle -- plastiq.js > plastiq.min.js"
